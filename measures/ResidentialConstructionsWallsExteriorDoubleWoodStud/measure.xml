<measure>
  <schema_version>3.0</schema_version>
  <name>process_constructions_walls_exterior_double_wood_stud</name>
  <uid>d3ae8875-36bc-4e4f-a5a9-c9d0dcd46651</uid>
  <version_id>f0c96dc8-03ce-4c9e-ae47-6f7cc5fcbd60</version_id>
